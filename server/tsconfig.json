{
    "compilerOptions": {
        "module": "amd",
        "lib": ["es2015", "dom"],
        "strictNullChecks": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "outFile": "static/script.js"
    },
    "include": [
        "../frontend/**/*.ts",
        "../frontend/*.ts"
    ]
}